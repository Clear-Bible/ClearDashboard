	<ItemGroup>
		<Reference Include="Caliburn.Micro.Core">
			<HintPath>..\Reference Assemblies\Caliburn.Micro.Core.dll</HintPath>
		</Reference>
		<Reference Include="Caliburn.Micro.Platform">
			<HintPath>..\Reference Assemblies\Caliburn.Micro.Platform.dll</HintPath>
		</Reference>
		<Reference Include="H.Formatters">
		  <HintPath>..\..\..\..\DotNET_Proj\H.Pipes\src\libs\H.Pipes\bin\Debug\netstandard2.0\H.Formatters.dll</HintPath>
		</Reference>
		<Reference Include="H.Formatters.BinaryFormatter">
		  <HintPath>..\..\..\..\DotNET_Proj\H.Pipes\src\libs\H.Pipes\bin\Debug\netstandard2.0\H.Formatters.BinaryFormatter.dll</HintPath>
		</Reference>
		<Reference Include="H.Pipes">
		  <HintPath>..\..\..\..\DotNET_Proj\H.Pipes\src\libs\H.Pipes\bin\Debug\netstandard2.0\H.Pipes.dll</HintPath>
		</Reference>
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\ClearDashboard.Common\ClearDashboard.Common.csproj" />
		<ProjectReference Include="..\ClearDashboard.DAL\ClearDashboard.DAL.csproj" />
	</ItemGroup>

	<ItemGroup>
		<Content Include="HelpFiles\NewProjectHelp_us.md">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</Content>
		<Resource Include="Resources\BiblicalTerms_logo_32.png">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</Resource>
		<Resource Include="Resources\ClearDashboard_logo_128.png">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</Resource>
		<Resource Include="Resources\NewProject_Icon_128.png">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</Resource>
		<Resource Include="Resources\NewProject_Icon_96.png">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</Resource>
		<Resource Include="Resources\Paratext_Connected32.png">
		  <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</Resource>
		<Resource Include="Resources\Paratext_Disconnected32.png">
		  <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</Resource>
		<Resource Include="Resources\settings_logo_128.png">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</Resource>
		<Resource Include="Resources\settings_logo_96.png">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</Resource>
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\ClearDashboard.Common\ClearDashboard.Common.csproj" />
		<ProjectReference Include="..\ClearDashboard.DAL\ClearDashboard.DAL.csproj" />
		<ProjectReference Include="..\pipes_shared\Pipes_Shared.csproj" />
	</ItemGroup>

	<ItemGroup>
		<Compile Update="Strings\Resources.Designer.cs">
		  <DesignTime>True</DesignTime>
		  <AutoGen>True</AutoGen>
		  <DependentUpon>Resources.resx</DependentUpon>
		</Compile>
		<Compile Update="Properties\Settings.Designer.cs">
			<DesignTimeSharedInput>True</DesignTimeSharedInput>
			<AutoGen>True</AutoGen>
			<DependentUpon>Settings.settings</DependentUpon>
		</Compile>
	</ItemGroup>

	<ItemGroup>
	  <EmbeddedResource Update="Strings\Resources.am.resx">
	    <Generator>PublicResXFileCodeGenerator</Generator>
	  </EmbeddedResource>
	  <EmbeddedResource Update="Strings\Resources.de.resx">
	    <Generator>PublicResXFileCodeGenerator</Generator>
	  </EmbeddedResource>
	  <EmbeddedResource Update="Strings\Resources.en.resx">
	    <Generator>PublicResXFileCodeGenerator</Generator>
	  </EmbeddedResource>
	  <EmbeddedResource Update="Strings\Resources.es.resx">
	    <Generator>PublicResXFileCodeGenerator</Generator>
