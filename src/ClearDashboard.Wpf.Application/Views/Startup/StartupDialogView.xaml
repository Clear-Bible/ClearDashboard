<Page x:Class="ClearDashboard.Wpf.Application.Views.Startup.StartupDialogView"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ClearDashboard.Wpf.Application.Views.Startup"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="StartupDialogView">

    <Grid>
        <!--<Button x:Name="GoToMain" cm:Message.Attach="GoToMain"  Background="{DynamicResource BlueDarkBrush}" Width="120">Go to Main</Button>-->
        <StackPanel
            Grid.Row="0"
            Grid.Column="0"
            Margin="10,10"
            HorizontalAlignment="Center"
            Orientation="Horizontal">
            <Button
                x:Name="Ok"
                Margin="5,5,5,5"
                Content="OK" />
            <Button
                x:Name="Cancel"
                Margin="5,5,5,5"
                Content="Cancel" />
        </StackPanel>
    </Grid>
</Page>
